#:import PlotterCanvas plottersim.widgets.plotter_canvas.PlotterCanvas
#:import Color kivy.graphics.Color


<PlotterCanvas>:
    canvas:
        Color:
            rgba: (246.0/255.0, 251.0/255.0, 247.0/255.0, 1)
        Rectangle:
            pos: self.x, self.y
            size: self.width, self.height
